  "packageManager": "pnpm@10.8.0",
  "engines": {
    "node": ">=22"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./cz.config.cjs"
